{
  "name": "quadrilateral",
  "version": "1.0.0-dev.76",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/quadrilateral.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "QUADRILATERAL",
    "simulation": true,
    "runnable": true,
    "generatedUnitTests": true,
    "supportedBrands": [
      "phet"
    ],
    "phetLibs": [
      "tangible"
    ],
    "preload": [
      "../sherpa/lib/mediaPipe/hands@0.4.1646424915/hands.js"
    ],
    "simFeatures": {
      "supportsInteractiveDescription": true,
      "supportsVoicing": true,
      "supportsSound": true,
      "supportsDynamicLocale": true
    },
    "supportsOutputJS": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "rules": {
      "todo-should-have-issue": "off"
    }
  }
}