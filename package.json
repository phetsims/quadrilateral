{
  "name": "quadrilateral",
  "version": "1.0.0-dev.46",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/quadrilateral.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "QUADRILATERAL",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "phetLibs": [
      "bamboo",
      "tappi",
      "tangible"
    ],
    "preload": [
      "../sherpa/lib/mediaPipe/hands@0.4.1646424915/mediaPipeDependencies.js",
      "../sherpa/lib/mediaPipe/hands@0.4.1646424915/hands.js",
      "../sherpa/lib/mediaPipe/camera_utils-0.3.1640029074.js",
      "../sherpa/lib/mediaPipe/control_utils-0.6.1629159505.js"
    ],
    "simFeatures": {
      "supportsInteractiveDescription": true,
      "supportsVoicing": true,
      "supportsSound": true
    },
    "supportsOutputJS": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/no-explicit-any": "error"
        }
      }
    ]
  }
}