{
  "name": "quadrilateral",
  "version": "1.0.0-dev.51",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/quadrilateral.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "QUADRILATERAL",
