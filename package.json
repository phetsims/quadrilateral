{
  "name": "quadrilateral",
  "version": "1.0.0-dev.12",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/quadrilateral.git"
  },
  "devDependencies": {
    "grunt": "~1.4.0"
  },
  "phet": {
    "requirejsNamespace": "QUADRILATERAL",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
